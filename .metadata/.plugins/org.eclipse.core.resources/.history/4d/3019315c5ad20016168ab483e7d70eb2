
public class PrintAmerican {
	
	public static void printAmerican(String day, int date, String month, int year) {
//		System.out.println("American Format:");
		System.out.print(day +  ", " + month + " " + date + ", " + year);
		System.out.println();
		
	}
	
	public static void printEuropean(String day, int date, String month, String year){
		System.out.println("Tomorrow's date in EU format:");
		System.out.println(day + " " + (++date) + " " + month + " " + year);
		System.out.println();
	}
	
	public static void main(String[] args) {
		printAmerican("Thursday", 19, "December", 2016);
		
	}
	
	
	
//	String day = "Thursday";
//	String month = "July";
//	int date = 16;
//	int year = 2016;
	
}
