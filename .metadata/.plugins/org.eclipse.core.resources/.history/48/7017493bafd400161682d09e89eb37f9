package StringTraversal;

public class reverseString {

	public static void main(String[] args) {

		String name1 = "name0";
		String name2 = "name1";
		
		//if name1 comes first alphabetically, returns -1. if comes second, returns 1.
		int diff = name1.compareTo(name2); //-1 becomes name1 comes first alphabetically
		
		
		//Determines which name comes first alphabetically 
		if (diff == 0) {
			System.out.println("Names are equal.");
		} else if (diff == -1) {
			System.out.println(name1 + " came first alphabetically before " + name2);
		} else if (diff == 1){
			System.out.println(name2 + " came first alphabetically after " + name1);
		}
		
		String myName = "Quan Truong";
		String yourName = "Alfred Quintero";
		
		int diffOfNames = myName.compareTo(yourName);
		
		System.out.println(diffOfNames);

	}
	
	

}
