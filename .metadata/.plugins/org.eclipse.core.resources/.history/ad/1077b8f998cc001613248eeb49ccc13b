package Exercises;

public class sqRt {

	public static void main(String[] args) {
		sqrt(9);
	
	}
	/**
	 * 
	 * @param numberToSquare Square root numberToSquareumber to be founumberToSquared
	 * @param a InumberToSquareitial guess
	 */
	public static void sqrt(double numberToSquare) {
		
		//inumberToSquareitial guess = a
		double a = numberToSquare/2;
		double i1 = (a + numberToSquare/a)/2;
		double i2 = (i1 + numberToSquare/i1)/2;
		while ( Math.abs(i1 - i2) >= 0.0001) {
			i1 = (i2 + numberToSquare/i2)/2;
			i2 = (i1 + numberToSquare/i1)/2;
		}
		System.out.println(i2);
		System.out.println(i1);
	}

}
